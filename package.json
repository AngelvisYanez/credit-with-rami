{
  "name": "credit-with-rami",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/network-info.js && next dev -H 0.0.0.0 -p 3000",
    "dev:network": "node scripts/network-info.js && next dev -H 0.0.0.0 -p 3000",
    "dev:local": "next dev -p 3000",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "node server.js",
    "start:next": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:network": "node scripts/network-info.js && next start -H 0.0.0.0 -p 3000",
    "start:production": "NODE_ENV=production node server.js",
    "network-info": "node scripts/network-info.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:status": "prisma migrate status",
    "deploy:prisma": "node scripts/setup-prisma.js",
    "verify:vercel": "node scripts/verify-vercel-setup.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.17.1",
    "express": "^4.18.2",
    "lucide-react": "^0.300.0",
    "next": "^14.2.0",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.4",
    "netlify-cli": "^23.8.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0"
  }
}
